<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='appCss' type='uk.cbooksys.client.AppCss' />
	<ui:style>
	.iconPanel {
		text-align: center;
		border: 0px solid;
		width: 100%;
	}
	
	.dotPanel {
		border: 0px solid;
		width: 100%;
		text-align: center;
/*		opacity: 0.2; */
	}
	</ui:style>
	<g:HTMLPanel width="100%" ui:field="cellPanel">
		
			<g:HTMLPanel ui:field="iconPanel" styleName="{style.iconPanel}"></g:HTMLPanel>
			<g:HorizontalPanel ui:field="courtPanel" styleName="{style.dotPanel}" horizontalAlignment="center" /> 
			<g:Label ui:field="cellText" visible="false" ></g:Label>

	</g:HTMLPanel>
</ui:UiBinder> 